{
  "name": "remot3-it-api",
  "version": "0.0.0",
  "description": "JavaScript wrapper for Remot3.it API",
  "main": "dist/index.js",
  "scripts": {
    "test": "NODE_ENV=test && mocha $(find src -name *.test.js) --compilers js:babel-register --require babel-polyfill",
    "test:w": "npm t -- --watch",
    "prebuild": "rimraf dist",
    "build": "npm run build:main && npm run build:umd && npm run build:umd.min",
    "build:main": "babel --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "prepublish": "npm run build"
  },
  "keywords": [
    "Remot3.it",
    "Weaved",
    "API wrapper"
  ],
  "files": [
    "dist",
    "readme.md"
  ],
  "author": "Erik Palla <hello@erikpalla.com> (http://erikpalla.com/)",
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "fetch-mock": "^5.12.2",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.1",
    "webpack": "^3.5.5"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erik-palla/remot3-it-api.git"
  },
  "homepage": "https://github.com/erik-palla/remot3-it-api#readme"
}